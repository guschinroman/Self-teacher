<Window
        ResxExtension.DefaultResxName="MainWindow"
        x:Class="SelfTeacher.WinApp.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525"
        Icon="../Assets/Images/Logo/logo-default.png">
    <Grid>
        <StackPanel>
            <ContentControl prism:RegionManager.RegionName="AuthRegion" />
            <ContentControl prism:RegionManager.RegionName="RegistrationRegion" />
            <ContentControl prism:RegionManager.RegionName="MainRegison" />
        </StackPanel>
    </Grid>
</Window>
